            <button class="btn waves-effect waves-light" type="submit" name="salvar">Salvar
<table class="striped centered">
   <thead>
      <tr>
         <th>Nome
         
         <th>E-mail
   
   <tbody>
      $forall (Entity usuid usuario) <- usuarios
         <tr>
            <td>
               #{usuarioNome usuario}
            <td>
               #{usuarioEmail usuario}
                  
      
